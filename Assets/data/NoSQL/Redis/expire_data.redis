import client from './redis'; // Assuming this file is in the same directory

// Set a key-value pair with an expiration time of 60 seconds
client.setex('my_key', 60, 'my_value', (err) => {
  if (err) {
    console.error('Error setting key:', err);
  } else {
    console.log('Key set with expiration');
  }
});
